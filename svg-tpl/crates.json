{
	"name": "crates",
	"label": "Crates",
	"view": {
		"parameters": {
			"scale": {
				"type": "range",
				"default": 1,
				"min": 0.1,
				"max": 4,
				"step": 0.1,
				"label": "Size"
			},
			"stroke_width": {
				"type": "range",
				"default": 2,
				"min": 0.1,
				"max": 20,
				"step": 0.1,
				"label": "Stroke Width"
			},
			"colorpanel": {
				"type": "colorpanel",
				"label": "Colors",
				"value": {
					"stroke": {
						"label": "Stroke",
						"default": "#000"
					},
					"color_1": {
						"label": "Color 1",
						"default": "#00f"
					},
					"color_2": {
						"label": "Color 2",
						"default": "#00c"
					},
					"color_3": {
						"label": "Color 3",
						"default": "#00a"
					}
				}
			}
		},
		"functions": {
			"svg_width": "return Math.round(120 * data.scale.value );",
			"svg_height": "return Math.round( 180 * data.scale.value );",
			"svg_stroke": "return ( data.colorpanel.value.stroke.value );",
			"c1": "return ( data.colorpanel.value.color_1.value );",
			"c2": "return ( data.colorpanel.value.color_2.value );",
			"c3": "return ( data.colorpanel.value.color_3.value );"
		}
	},
	"presets": [
		{
			"label": "metalic",
			"view": {
				"scale": 1,
				"stroke_width": 1,
				"colorpanel": {
					"stroke": "#000",
